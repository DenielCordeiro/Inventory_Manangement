{
  "name": "field_manangement",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm dev:server\" \"pnpm dev:ts\"",
    "dev:server": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:ts": "tsc --watch",
    "sass": "sass public --no-source-map --watch",
    "build": "pnpm run -s build:ts build:sass",
    "build:ts": "tsc",
    "build:sass": "sass public --no-source-map --style=compressed"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "concurrently": "^9.2.1",
    "sass": "^1.70.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "express": "^5.2.1"
  }
}
